# .env.example
# Копируйте этот файл в .env и заполните значения

# Яндекс.Диск OAuth токен
# Инструкция: https://yandex.ru/dev/id/doc/ru/
YANDEX_DISK_TOKEN=your_yandex_oauth_token_here

# MLFlow tracking URI
MLFLOW_TRACKING_URI=http://localhost:5000

# Путь к модели
MODEL_PATH=./models/jdаs_final.pth

# Пути к данным
DATA_PATH=./data/raw
OUTPUT_PATH=./output

# Уровень логирования (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
